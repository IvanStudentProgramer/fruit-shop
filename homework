fruit = (input())
day_of_week = str(input())
amount = float(input())
price = 0

banana_price = 2.50
apple_price = 1.20
orange_price = 0.85
grapefruit_price = 1.45
kiwi_price = 2.70
pineapple_price = 5.50
grapes_price = 3.85

banana_price_week = 2.70
apple_price_week = 1.25
orange_price_week = 0.90
grapefruit_price_week = 1.60
kiwi_price_week = 3
pineapple_price_week = 5.60
grapes_price_week = 4.20

if day_of_week == 'Monday' or day_of_week == 'Tuesday' or day_of_week == 'Wednesday'\
        or day_of_week == 'Thursday' or day_of_week == 'Friday':
    if fruit == 'banana':
        price = amount * banana_price
    elif fruit == 'apple':
        price = amount * apple_price
    elif fruit == 'orange':
        price = amount * orange_price
    elif fruit == 'grapefruit':
        price = amount * grapefruit_price
    elif fruit == 'kiwi':
        price = amount * kiwi_price
    elif fruit == 'pineapple':
        price = amount * pineapple_price
    elif fruit == 'grapes':
        price = amount * grapes_price

if day_of_week == 'Sunday' or day_of_week == 'Saturday':
    if fruit == 'banana':
        price = amount * banana_price_week
    elif fruit == 'apple':
        price = amount * apple_price_week
    elif fruit == 'orange':
        price = amount * orange_price_week
    elif fruit == 'grapefruit':
        price = amount * grapefruit_price_week
    elif fruit == 'kiwi':
        price = amount * kiwi_price_week
    elif fruit == 'pineapple':
        price = amount * pineapple_price_week
    elif fruit == 'grapes':
        price = amount * grapes_price_week

if price == 0:
    print('error')
else:
    print(f"{price:.2f}")
